<h1>User List</h1>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Role</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody id="user-table-body">
      <% users.forEach(user => { %>
        <tr data-id="<%= user._id %>">
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.phone %></td>
          <td><%= user.role %></td>
          <td>
            <button class="edit-btn">Edit</button>
            <button class="delete-btn">Delete</button>
          </td>
        </tr>
      <% }); %>
    </tbody>
  </table>


  <h1>Upload Users Excel File</h1>
  <form action="/administrator/upload-users-excel" method="post" enctype="multipart/form-data">
      <input type="file" name="file" accept=".xlsx, .xls">
      <button type="submit">Upload</button>
  </form>